{
  "name": "osudirect-desktop",
  "version": "0.0.1",
  "description": "Desktop app for osu.direct",
  "main": "dist/main.js",
  "type": "commonjs",
  "scripts": {
    "start": "tsx start.ts",
    "compile": "tsx compile.ts",
    "linux:build": "rm -rf dist/ && tsc",
    "linux:start": "npm run linux:build && electron ./dist/main.js",
    "linux:compile": "npm run linux:build && electron-builder",
    "win:build": "rmdir /s /q dist > nul && tsc",
    "win:start": "npm run win:build && electron ./dist/main.js",
    "win:compile": "npm run win:build && electron-builder",
    "watch": "tsc -w",
    "lint": "eslint -c .eslintrc --ext .ts ./src"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "electron": "^25.3.0",
    "eslint": "^7.32.0",
    "typescript": "^5.1.6",
    "electron-reload": "^2.0.0-alpha.1"
  },
  "dependencies": {
    "custom-electron-titlebar": "^4.2.3"
  }
}